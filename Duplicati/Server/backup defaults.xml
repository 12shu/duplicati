<?xml version="1.0" encoding="utf-8" ?>
<!-- 

This file contains built-in defaults.
To override or suppply custom default settings,
put a new file into the server root folder,
and name it "backup defaults.xml".

-->
<settings>
  <Repeat>1D</Repeat>
  <When>13:00:00</When>

  <Task>
    <EncryptionModule>aes</EncryptionModule>
    <CompressionModule>zip</CompressionModule>
    
    <Extensions>
	    <SelectFiles_IncludeDocuments>true</SelectFiles_IncludeDocuments>
	    <SelectFiles_IncludeDesktop>true</SelectFiles_IncludeDesktop>
	    <SelectFiles_IncludeMusic>false</SelectFiles_IncludeMusic>
	    <SelectFiles_IncludeImages>true</SelectFiles_IncludeImages>
	    <SelectFiles_IncludeAppData>false</SelectFiles_IncludeAppData>
		<DisableAESFallbackDecryption>true</DisableAESFallbackDecryption>
		<VolumeSize>50MB</VolumeSize>
	</Extensions>
	
    <IncludeSetup>true</IncludeSetup>
	<KeepFull>4</KeepFull>
	<FullAfter>1M</FullAfter>
	
	<Filters>
		<Exclude>*\Thumbs.db</Exclude>
	</Filters>
	
  </Task>

</settings>